<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.liuyiyou.shop.base.mapper.BannerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.liuyiyou.shop.base.entity.Banner">
        <id column="banner_id" property="bannerId"/>
        <result column="pic" property="pic"/>
        <result column="target_url" property="targetUrl"/>
        <result column="weight" property="weight"/>
        <result column="create_date" property="createDate"/>
        <result column="last_update" property="lastUpdate"/>
        <result column="width" property="width"/>
        <result column="height" property="height"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        banner_id, pic, target_url, weight, create_date, last_update, width, height
    </sql>

</mapper>
